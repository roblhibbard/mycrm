<div class="col-xs-12 col-md-12">
 <div class='nested-fields'>
   <div class="row">
	 
         <div class="col-xs-1 col-md-1"><%= f.input :qty, label: false %></div>
         <div class="col-xs-4 col-md-4 mb-1"><%= f.collection_select :service_list_id, service_list_charge, :id, :name, :include_blank => true, prompt: "Choose Service" %></div>
         <div class="col-xs-5 col-md-5"><%= f.input :line_comment, placeholder: "Comment", label: false %></div>   
         <div class="col-xs-2 col-md-2"><%= link_to_remove_association "Remove Line", f, :class => 'remove_fields btn btn-small btn-danger', style: "width: %" %></div>
     </div> 
   </div>
 </div>  

  
        <%= f.hidden_field :client_id, value: session[:client] %>   
