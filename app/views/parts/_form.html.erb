<%= simple_form_for(@part) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <%= f.input :price %>
    <%= f.input :parts_on_order %>
    <%= f.input :parts_in %>
    <%= f.input :date_ordered %>
    <%= f.input :date_received %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
