<p id="notice"><%= notice %></p>

<h1>Lab Ticket Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Lab status</th>
      <th>Labticket</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lab_ticket_statuses.each do |lab_ticket_status| %>
      <tr>
        <td><%= lab_ticket_status.name %></td>
        <td><%= lab_ticket_status.lab_status %></td>
        
        <td><%= link_to 'Show', lab_ticket_status %></td>
        <td><%= link_to 'Edit', edit_lab_ticket_status_path(lab_ticket_status) %></td>
        <td><%= link_to 'Destroy', lab_ticket_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lab Ticket Status', new_lab_ticket_status_path %>
