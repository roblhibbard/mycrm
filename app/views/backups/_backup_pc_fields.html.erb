     <div class="col-xs-12 col-md-12">
     	
     <div class="nested-fields">
     	<div class="row">
     
         <div class="col-xs-1 col-md-4"><%= f.collection_select :labtech_computer, backup_computer, :id, :name, :include_blank => true, prompt: "Bus Computer" %></div>
         <div class="col-xs-1 col-md-4"><%= f.collection_select :computer_id, res_computers, :id, :corepc_number, :include_blank => true, prompt: "Res Computer" %></div>
         <div class="col-xs-1 col-md-4"><%= f.input :name, placeholder: "Computer Name", label: false %></div>
         <div class="col-xs-1 col-md-2"><%= f.input :active %></div>
         <div class="col-xs-1 col-md-5"><%= f.input :external_hd, placeholder: "External HD", label: false %></div>
         <div class="col-xs-1 col-md-5"><%= f.input :serial, placeholder: "Serial #",label: false %></div>
         <div class="col-xs-1 col-md-6"><%= f.input :notes, placeholder: "Comment", label: false %></div>   
         <div class="col-xs-1 col-md-3 p-2"><%= link_to_remove_association "Remove Line", f, :class => 'remove_fields btn btn-small btn-danger', style: "width: %" %></div>
       
       </div> 
    </div>
    </div>    
    
    
 
        <%= f.hidden_field :client_id, value: session[:client] %>   
