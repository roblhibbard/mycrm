<p id="notice"><%= notice %></p>
	<div class="table-row header hidden-xs-down">      	      
	      <div class="col-xs col-md-2">Backup Acct</div>
	      <div class="col-xs col-md-1">ID</div>
	      <div class="col-xs col-md-2">Client</div>
	      <div class="col-xs col-md-1">Size</div>
          <div class="col-xs col-md-2">Service list</div>
	      <div class="col-xs col-md-2">Signup Date</div>
	 </div>     
    <% @backups.each do |backup| %>
     <div class="table-row mb-2 ">  
      	<div class="col-xs col-md-2"><%= backup.backup_account %></div>
      	<div class="col-xs col-md-1"><%= link_to backup.id, backup  %></div>
      	<div class="col-xs col-md-2"><%= link_to backup.full_name, client_path(backup.client_id) %></div>    
        <div class="col-xs col-md-1"><%= backup.backup_size %></div>
        <%if backup.service_list.blank? %>
        <div class="col-xs col-md-2"> N/A </div>
        <%else%>
        <div class="col-xs col-md-2"><%= backup.service_list.name %></div>
        <%end%>
        <div class="col-xs col-md-2"><%= backup.signup_date %></div>
        
     </div>   
     
    <% end %>
 





