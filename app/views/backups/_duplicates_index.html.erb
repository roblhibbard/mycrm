
   <div class="flex-margin">
   	
   	<div class="align-center">
   	  <%= form_tag(backups_path, method: "get" , id: "search-form") do %>
      <%= label_tag(:search, "Search for:") %>
      <%= text_field_tag :search, params[:search], palceholder: "Search Backups" %>
      <%= submit_tag "Search", :name => nil %>
      <% end %> </div>
      <p id="notice"><%= notice %></p>
   	
	<div class="align-center"> <%= paginate @backups %></div>
  	<div class="table table-striped">
  	<table class="table table-sm">
      <thead>
	    <tr>
	      	     
	      <th>Backup Acct</th>
	      <th>BackupID</th>
	      <th>Client Name</th>
	      <th>Email</th>
	      <th>Phone#</th>
	      <th>Full Name</th>
	     
	     
	      
	    </tr>
	  </thead>

  <tbody>
    <% @duplicate_backups.each do |backup| %>
      <tr>  
      	<td><%= backup.backup_account %></td>
      	<td><%= link_to backup.id, backup  %></td>
      	
      	<td><%= link_to backup.full_name, client_path(backup.client_id) %></td>    
        <td><%= mail_to backup.client.email  %></td>
        <td><%= backup.client.phone_number  %></td>
        <td><%= backup.client_full_name %></td>
        
        
        
      </tr>
      
    <% end %>
  </tbody>
</table>
	<div class="align-center"> <%= paginate @backups %> </div>
		</div>
		</div>



