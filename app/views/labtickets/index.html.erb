<p id="notice"><%= notice %></p>
<div class="container-fluid p-4">
	
<div class="align-center"><%= render partial: 'search_client' %> </div>
	
<h3>Labtickets</h3>

<div class="col-xs col-md-12">
<div class="table-row header">
	
      <div class="col-xs col-md-1">TicketID</div>
      <div class="col-xs col-md-1">ClientID</div>
      <div class="col-xs col-md-2">Client</div>
      <div class="col-xs col-md-2">Phone</div>
       
      <div class="col-xs col-md-4">Subject</div>
       
      
      
   </div> 

 
    <% @tickets['value'].each do |key| %>
     <div class="table-row">	
        <div class="col-xs col-md-1"><%= key['TicketID'] %></div>
         <div class="col-xs col-md-1"><%= key['ClientID'] %></div>
         <% if key['Client'].nil? %> "Blank"
         <% else %>
          <div class="col-xs col-md-2"><%= key['Client']['Name'] %></div>
          <% end %>
          <% if key['Client'].nil? %> "Blank"
          <% else %>
          <div class="col-xs col-md-2"><%= key['Client']['Phone'] %></div>
           <% end %>
          <div class="col-xs col-md-4"><%= key['Subject'] %></div>
          
       </div> 
        
    
    <% end %>
   </div> 

<div><%= render partial: 'offline_servers' %> </div>
<div><%= render partial: 'critical_drive' %> </div>
<div><%= render partial: 'hd_space' %> </div>
<div><%= render partial: 'QBR' %> </div>


<div class="col-xs-3 p-1"><%= link_to 'Create LabTicket', ticket_labtickets_path, class: "btn btn-primary" %> </div>
<div class="align-center m-1"><%= link_to 'print open_tickets', open_tickets_labtickets_path, object: @open_tickets, class: "btn btn-primary" %></div>
<div class="align-center m-1"><%= link_to 'print QBR_tickets', qbr_tickets_labtickets_path, object: @qbr_tickets, class: "btn btn-primary" %></div>	
<div class="align-center m-1"><%= link_to 'print Critical Drives', critical_drive_labtickets_path, object: @critical_drive, class: "btn btn-primary" %></div>
<div class="align-center m-1"><%= link_to 'Print Printer Tickets', printer_tickets_labtickets_path, object: @printer_tickets, class: "btn btn-primary" %></div>
<div class="align-center m-1"><%= link_to 'Extra Client Data', extra_data_labtickets_path, object: @results, class: "btn btn-primary" %></div>
<div class="align-center m-1"> <%= link_to 'Client Ticket Report', c_tickets_labtickets_path, object: @result, class: 'btn btn-primary' %></div>	
</div>
