<div class="container-fluid">
<%= simple_form_for :make_labticket, :url => url_for(:action => 'make_labticket', :controller => 'labtickets'),
                     :method => 'post', html: { class: 'form-horizontal' }  do |f| %>
  <%= f.error_notification %>

 <div class="row">
  
  	<div class="col-xs px-5">
    <%= f.input :labtech_client, collection: LabtechClient.order(:name), label_method: :name, value_method: :lclient_id.to_s, prompt:
                     "Choose Client", include_blank: false %>
                     
    <%= f.input :requestor_email %>
    <%= f.input :labtechloc %> 
    <%= f.input :labtechpc %>                
    <%= f.input :labtech_id, collection: User.order(:email), label_method: :email, value_method: :labtech_id.to_s, prompt:
                     "Choose Tech", include_blank: false %>
    <%= f.input :started_date, as: :datetime, html5: true%>
    <div class="col-xs px-5">
    <%= f.input :subject %>	
  	
       
   <%= f.button :submit, class: "btn btn-primary form-control" %><br></br>
  </div>
  </div>
 
  </div>
</div>
<% end %>
