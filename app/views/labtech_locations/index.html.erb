<p id="notice"><%= notice %></p>

<h1>Labtech Locations</h1>

<table>
  <thead>
    <tr>
      <th>LOC ID</th>
      <th>ClientID</th>
     
       <th>Name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Fax</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @labtech_locations.each do |labtech_location| %>
      <tr>
      	<td><%= link_to labtech_location.id, labtech_location %></td>
      	<td><%= link_to labtech_location.labtech_client_id, labtech_client_path(labtech_location.labtech_client_id) %></td>
        <td><%= labtech_location.name %></td>
        <td><%= labtech_location.address %></td>
        <td><%= labtech_location.city %></td>
        <td><%= labtech_location.state %></td>
        <td><%= labtech_location.zip %></td>
        <td><%= labtech_location.phone %></td>
        <td><%= labtech_location.fax %></td>
        <td><%= labtech_location.comments %></td>
        <td><%= link_to 'Show', labtech_location %></td>
        <td><%= link_to 'Edit', edit_labtech_location_path(labtech_location) %></td>
        <td><%= link_to 'Destroy', labtech_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Labtech Location', new_labtech_location_path %>
<%= link_to 'Sync Locations', import_location_labtech_locations_path %>

